<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="PartnerNet.Domain" namespace="PartnerNet.Domain">
  <class name="ProductViewScala" table="ProductViewScala" >
    <id name="Id" type="System.Int32" column="Id">
      <generator class="identity"/>
    </id>
    <property name="Price" column="Price" type="System.Double" not-null="true" />
    <property name="Currency" column="Currency" type="System.String" not-null="true" />
    <property name="CurrentStock" column="CurrentStock" type="System.Int32" not-null="true" />
    <property name="ReservedStock" column="ReservedStock" type="System.Int32" not-null="true" />
    <property name="OrderedStock" column="OrderedStock" type="System.Int32" not-null="true" />
  </class>
</hibernate-mapping>

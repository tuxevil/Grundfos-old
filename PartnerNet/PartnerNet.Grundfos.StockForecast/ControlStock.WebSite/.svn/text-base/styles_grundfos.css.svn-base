@charset "utf-8";
.fdo_header {
	background-image: url(images/fdo_header.gif);
	background-repeat: no-repeat;
}
.fdo_footer {
	background-image: url(images/fdo_footer.gif);
	background-repeat: no-repeat;
}
.fdo_tit {
	background-image: url(images/fdo_tit.gif);
	background-repeat: repeat-x;
}
.text1 {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #0e4b88;
}
.fdo_controles {
	background-image: url(images/control_fdo.gif);
	background-repeat: no-repeat;
	background-position: center;
}

.paginado {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	font-weight: normal;
	text-decoration: none;
	color: #000000;
}
.text2 {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	font-weight: bold;
	color: #FFFFFF;
}
.text3 {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #e1861b;
	font-weight: bold;
}
.text4 {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	font-weight: bold;
	color: #000000;
}
.textlink {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	font-weight: bold;
}
.text5 {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	font-weight: normal;
	text-transform: uppercase;
	color: #4b7fa6;
}

.paginado:hover {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	font-weight: bold;
